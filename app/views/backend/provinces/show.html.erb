<div class="box box-danger">
  <div class="box-header">
    <i class="ion ion-clipboard"></i>
    <h2 class="box-title"><%= @province.name %></h2>
    <div class="pull-right">
      <%= link_to "Edit", edit_backend_province_path(@province), class: 'btn btn-success' %>
      <%= link_to "Delete", backend_province_path(@province), method: :delete, data: {confirm: "Are You Sure Delete?"}, class: 'btn btn-danger' %>
    </div>
  </div>
  <div class="box-body">
    <ul class="todo-list ui-sortable-handle">
      <th>
        Nama
      </th>
      <li><%= @province.name %></li>
    </ul>
  </div>
</div>
